{
   
    inlet_A
    {
       type             fixedValue;
        value           uniform 0;
    }
    inlet_B
    {
       type             fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    
   
    walls
    {
        type            fixedFluxExtrapolatedPressure;
        value           uniform 0;
    }
	
	 axV
    {
        type            fixedFluxExtrapolatedPressure;
        value           uniform 0;
    }

}

