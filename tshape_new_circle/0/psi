FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      psi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 2 -3 0 0 -1 0];

internalField   uniform 0;

boundaryField
{

    inlet_A
    {
       type            zeroGradient;
    }
    
    inlet_B
    {
      type            zeroGradient;
    }

    walls
    {
        type            fixedValue;
        value           uniform -0.14;
    }

    outlet
    {
         type            zeroGradient;
    }
	
    axV
    {
        type            fixedValue;
        value           uniform -0.14;
    }
 
    
}

